{
    "name": "MuteMeet",
    "version": "1.0.2",
    "manifest_version": 3,
    "description": "Makes Video and Microphone Mute Automatically",
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}